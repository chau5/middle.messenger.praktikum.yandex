<div class="window lg single-chat bg-orange-light d-flex h-100">
    <div class="inner d-flex flex-column">
        {{{props.header}}}
        <div class="messages overflow-scroll-y h-100 mb-3">
            {{{props.date}}}
            {{#each props.messages}}
                {{{this.message}}}
            {{/each}}
        </div>
        {{{props.footer}}}
    </div>
</div>